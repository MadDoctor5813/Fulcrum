<Window x:Class="Fulcrum.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fulcrum"
        mc:Ignorable="d"
        Title="Fulcrum" Height="137" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnCreate" Content="Create" Margin="10" Click="btnCreate_Click"/>
        <Button x:Name="btnExtract" Content="Extract" Grid.Column="1" Margin="10" Click="btnExtract_Click"/>
    </Grid>
</Window>
